{
  "name": "zeedown",
  "description": "Tiny markdown renderer for Node and browser, with CLI.",
  "version": "4.1.3",
  "author": "Zac Anger <zac@zacanger.com> (http://zacanger.com)",
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/zacanger"
  },
  "license": "LGPL-3.0",
  "main": "index.js",
  "files": [
    "cli.js",
    "index.js"
  ],
  "bin": "./cli.js",
  "scripts": {
    "preversion": "npm t",
    "test": "run-s -s test:lint test:cover",
    "test:cover": "nyc npm run test:tape",
    "test:lint": "eslint -c .eslintrc.json *.js",
    "test:tape": "tape index.test.js"
  },
  "homepage": "https://github.com/zacanger/zeedown#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zacanger/zeedown.git"
  },
  "bugs": {
    "url": "https://github.com/zacanger/zeedown/issues"
  },
  "keywords": [
    "cli",
    "markdown",
    "md",
    "renderer"
  ],
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-plugin-zacanger": "1.1.4",
    "husky": "4.2.5",
    "lint-staged": "10.1.3",
    "npm-run-all": "4.1.5",
    "nyc": "15.0.1",
    "prettier": "2.0.4",
    "sortpack": "2.1.4",
    "tape": "4.13.2"
  },
  "typings": "zeedown.d.ts",
  "engines": {
    "node": ">=6.4.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "package.json": [
      "sortpack"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "nyc": {
    "all": true,
    "include": [
      "index.js"
    ],
    "reporter": [
      "text",
      "html",
      "json"
    ]
  }
}
